{
  "name": "driveinthedesert",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server",
    "build": "npm run clean && webpack -p",
    "clean": "rm -rf dist",
    "deploy_old": "git add dist && git commit -a -m 'Deployed!' && git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "sync:s3": "AWS_DEFAULT_PROFILE=cezar aws s3 sync dist s3://driveinthedesert.com && AWS_DEFAULT_PROFILE=cezar aws s3 sync dist s3://www.driveinthedesert.com",
    "sync:cloudfront": "AWS_DEFAULT_PROFILE=cezar aws cloudfront create-invalidation --distribution-id E3IBDZTTC6CYE1 --paths \"/*\"",
    "deploy": "npm run build && npm run sync:s3 && npm run sync:cloudfront"
  },
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "less": "^3.8.1",
    "navigo": "^7.1.2",
    "pify": "^3.0.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "less-loader": "^4.1.0",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "peerDependencies": {
    "less": "^3.8.1"
  }
}
